#include<stdio.h>

int main()
{
    int N;                     // Declare variable for number of rows
    scanf("%d", &N);           // Read input N from the user

    // Loop for each row from 1 to N
    for(int i = 1; i <= N; i++){
        // Loop to print numbers from 1 to i in each row
        for(int j = 1; j <= i; j++){
            printf("%d ", j);  // Print numbers in the row
        }
        printf("\n");          // Move to the next row after finishing current row
    }

    return 0;                  // End of program
}
