#include<stdio.h>                      // Standard input/output header file include

int main()
{
    int X;                             // Declare integer variable X to store user input

    for(int i = 1000; i <= 9999; i++){ // Loop runs for values from 1000 to 9999 (max 9000 times)
        scanf("%d", &X);               // Take input from user

        if(X == 1999){                 // Check if input equals 1999
            printf("Correct\n");       // If yes, print "Correct"
            break;                     // Exit the loop immediately (stop asking for input)
        }
        else{                          // If input is not 1999
            printf("Wrong\n");         // Print "Wrong"
        }
    }
    
    return 0;                          // End of the program
}


