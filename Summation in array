#include <stdio.h>
#include <stdlib.h>
int main()
{
    int n;
    scanf("%d", &n);
    long long int arr[n], sum = 0;
    for (int i = 0; i < n; i++)
    {
        arr[i] = 0;
        scanf("%lld", &arr[i]);
        sum = sum + arr[i];
    }
    if(sum<0){
        sum=sum*=-1;
    }
    printf("%lld", sum);
    return 0;
}
