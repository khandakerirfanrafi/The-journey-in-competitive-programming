#include<stdio.h>
int main()
{
    long long int A,B;  //declear 2 integer number
    scanf("%lld %lld",&A,&B);  //taking input from user
    printf("%lld + %lld = %lld\n",A,B,A+B); //summation
    printf("%lld * %lld = %lld\n",A,B,A*B); //multiplication
    printf("%lld - %lld = %lld\n",A,B,A-B);  //substraction
    return 0;
}
