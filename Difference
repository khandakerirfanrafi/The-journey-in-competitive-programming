#include<stdio.h>
int main()
{
    long long int A,B,C,D,X;  //declear 4 integer number
    scanf("%lld %lld %lld %lld",&A,&B,&C,&D); //take input 4 integer from user
    X=(A*B)-(C*D);
    printf("Difference = %lld",X);
    return 0;
}
