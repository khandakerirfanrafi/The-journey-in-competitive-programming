{
    int A,B,C,D,X;  //declear 4 integer number
    scanf("%d %d %d %d",&A,&B,&C,&D);   //take input 4 integer from user
    X=(A*B)-(C*D);       
    printf("Difference â€‰= %d",X); 
    return 0;
}

