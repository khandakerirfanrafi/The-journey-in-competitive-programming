#include<stdio.h>
int main(){
    int N;            // declear an integer
    scanf("%d",&N);  //input from user
    for(int i=1;i<=N;i++){       //using loop structure and this is for loop(initial statement, condition, updation)
        printf("%d\n",i);         // loop body
    }
    return 0;                   // end the code
}


