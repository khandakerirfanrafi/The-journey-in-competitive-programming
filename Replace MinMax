#include <stdio.h>
#include <limits.h>

int main() {
    int n;
    scanf("%d", &n);
    int arr[n], max = INT_MIN, min = INT_MAX;
    int z, j, s; // z = current value, j = min index, s = max index

    // Input array elements
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Find min and max values with their positions
    for (int i = 0; i < n; i++) {
        z = arr[i]; // current element
        if (max < z) { // check for max
            max = z;
            s = i; // store max index
        }
        if (min > z) { // check for min
            min = z;
            j = i; // store min index
        }
    }

    // Swap min and max
    int temp = arr[j];
    arr[j] = arr[s];
    arr[s] = temp;

    // Print updated array
    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}


