#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n, m;
    scanf("%d %d", &n, &m);
    int a[n][m];
    if (n == m){
        int flag = 100;
        for (int i = 0; i < n; i++){
            for (int j = 0; j < m; j++){
                scanf("%d", &a[i][j]);
                if (i == j){
                    if (a[i][j] != 1){
                        flag = 200;
                        printf("NO\n");
                        return 0;
                    }
                }
                if (n - 1 == i + j){
                    if (a[i][j] != 1){
                        flag = 200;
                        printf("NO\n");
                        return 0;
                    }
                }
                if (i != j && n - 1 != i + j){
                    if (a[i][j] != 0){
                        flag = 200;
                        printf("NO\n");
                        return 0;
                    }
                }
            }
            
        }
        if(flag==100){
            printf("YES\n");
        }
    }
    else{
        printf("NO\n");
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
